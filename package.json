{
  "name": "tizenos-react-sample",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "format": "prettier --write .",
    "build:tizen": "yarn build && yarn prepare:tizen",
    "prepare:tizen": "yarn copy:config && yarn copy:assets",
    "copy:config": "copyfiles -f config.xml dist",
    "copy:assets": "copyfiles -f icon.png dist",
    "package": "yarn build:tizen && yarn wgt:create",
    "wgt:create": "tizen build-web -out dist -- . && tizen package --type wgt --sign tizenos-key -- dist",
    "deploy": "yarn package && yarn install && yarn run",
    "install": "sdb install tizenos-react-sample.wgt",
    "run": "sdb shell app_launcher -s com.example.tizenossample",
    "clean": "rm -rf dist tizenos-react-sample.wgt"
  },
  "dependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "babel-plugin-react-compiler": "^1.0.0",
    "copyfiles": "^2.4.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "prettier": "^3.7.4",
    "terser": "^5.44.1",
    "vite": "yarn:rolldown-vite@7.2.5"
  },
  "resolutions": {
    "vite": "yarn:rolldown-vite@7.2.5"
  },
  "packageManager": "yarn@4.12.0"
}
